{
  "name": "refactoring-workshop",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@testing-library/react": "^9.3.0",
    "react-scripts": "3.2.0"
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "typescript": "^3.4.4"
  },
  "scripts": {
    "api": "npx json-server --watch db.json --port 3009 --middlewares ./mock/available.js",
    "test:coverage": "npm test -- --coverage --coverageReporters=html --watchAll=false && open coverage/index.html",
    "lint": "npx eslint ./",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/*.js"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
